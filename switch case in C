#include<stdio.h>
#include<conio.h>
int main()
{
    printf("Enter value: \n1. 0\n2. 1\n");
    int x=0;
    scanf("%d",&x);

    switch(x)
    {
    case 1:
        printf("Choice is 0\n");
        break;
    case 2:
        printf("Choice is 1\n");
        break;
    default: printf("Invalid coice");
    }
    getch();
    return 0;
}
